{
  "name": "firebase-hackernews",
  "version": "2.9.0",
  "description": "Hacker News APIs with firebase",
  "license": "MIT",
  "repository": "ragingwind/firebase-hackernews",
  "author": {
    "name": "ragingwind",
    "email": "ragingwind@gmail.com",
