    "deploy": "now --name=ruby-pwa --public",
    "remove": "now rm ruby-pwa --safe"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "emotion": "^8.0.12",
    "firebase": "^4.1.2",
    "firebase-hackernews": "^2.9.0",
    "isomorphic-fetch": "^2.2.1",
    "next": "4.0.0-beta.1",
    "prop-types": "^15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "theming": "^1.3.0",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "express": "^4.15.3",
    "globby": "^6.1.0",
    "load-json-file": "^2.0.0",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "workbox-build": "^1.0.1",
    "workbox-webpack-plugin": "^1.0.1",
    "xo": "^0.18.2"
  },
  "xo": {
    "semicolon": false,
    "env": {
      "browser": true
    },
