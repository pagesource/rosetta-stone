{
  "name": "@eXTract/ruby",
  "version": "0.0.1",
  "description": "A mobile-first e-commerce PWA.",
  "author": "agoel43@sapient.com",
  "license": "ISC",
  "keywords": [
    "react",
    "next.js",
    "firebase",
    "workbox",
    "e-commerce",
    "pwa"
  ],
  "engines": {
    "node": "^8.1.0"
  },
  "scripts": {
    "lint": "xo",
    "dev": "next",
    "build": "next build && node generate-sw.js",
    "report": "npm run build --report && cat .next/stats.txt",
    "start": "NODE_ENV=production node server.js",
